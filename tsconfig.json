{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6",
         "es2018", 
         "esnext.asynciterable"
      ],
      "target": "es2018",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "declaration": false,
   }
}